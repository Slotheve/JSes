#!name=Flow statistics
#!desc=流量统计

[Panel]
#流量统计
TrafficStatistics = script-name=TrafficStatistics,update-interval=1

[Script]
#流量统计
#应当修改的字段：network : en0→WiFi pdp_ip0→卡1 pdp_ip1→卡2
TrafficStatistics = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/Slotheve/Surge/main/Module/statistics.js,argument=icon=arrow.up.arrow.down.circle&color=#5d84f8&network=pdp_ip0
